{
  "name": "bible-text",
  "displayName": "Bible Text",
  "description": "",
  "publisher": "DominicVonk",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:bible-text.previous",
    "onCommand:bible-text.bless",
    "onCommand:bible-text.restart"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bible-text.previous",
        "title": "Get previous Bible Verse"
      },
      {
        "command": "bible-text.bless",
        "title": "Get new Bible Verse"
      },
      {
        "command": "bible-text.restart",
        "title": "Bible Verse service restart"
      }
    ],
    "configuration": {
      "title": "Bible Text",
      "properties": {
        "Bible Text.duration": {
          "type": "integer",
          "default": 1,
          "description": "How often to show the Bible verse? (in minutes)"
        },
        "Bible Text.translation": {
          "type": "string",
          "default": "NIV",
          "description": "Which Bible translation to use?",
          "enum": [
            "NIV",
            "KJV",
            "BBE"
          ]
        },
        "Bible Text.show on startup": {
          "type": "boolean",
          "default": true,
          "description": "Show a message on VS Code startup?"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
